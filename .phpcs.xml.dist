<?xml version="1.0"?>
<ruleset name="Croogo">
    <description>Croogo</description>

    <arg name="colors"/>
    <arg name="parallel" value="75"/>
    <arg value="ps"/>
    <arg name="extensions" value="php,ctp"/>

    <file>./</file>

    <exclude-pattern>vendor/*</exclude-pattern>

    <rule ref="vendor/cakephp/cakephp-codesniffer/CakePHP"/>
</ruleset>
